{
  "data": {
    "url": "http://localhost:9200/kibana_sample_data_ecommerce/_search?q=order_date:>now-2d+AND+order_date:<now-1d",
    "format": {
      "type": "json",
      "property": "hits.hits"
    }
  },
  "mark": "bar",
  "encoding": {
    "y": {
      "field": "_source.geoip.continent_name",
      "type": "ordinal",
      "title": "By continent"
    },
    "x": {
      "field": "_source.taxful_total_price",
      "aggregate": "sum",
      "type": "quantitative",
      "title": "Sales yesterday"
    }
  }
}